{
  "name": "lambda-boilerplate",
  "version": "0.1.0",
  "description": "Reference implementation and starting point for Lambda projects.",
  "author": "MSGTech",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://git@github.com:MSGTech/Lambda-Node-Boilerplate.git"
  },
  "keywords": [
    "lambda",
    "serverless"
  ],
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^0.3.3",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "webpack": "^1.12.11",
    "webpack-closure-compiler": "^2.1.4",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {},
  "main": "index.js",
  "scripts": {
    "build": "concurrently \"npm run lint\" \"npm test\" \"webpack --progress --colors\"",
    "deploy:dev": "cd dist && sls deploy --stage dev --region us-east-1 -v",
    "deploy:prod": "cd dist && sls deploy --stage prod --region us-east-1 -v",
    "lint": "eslint src/",
    "setup": "npm i serverless aws-sdk -g & npm i",
    "test": "mocha-webpack --webpack-config webpack.config.test.js \"src/**/*.test.js\""
  }
}
